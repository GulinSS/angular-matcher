.angular-matcher.input-xxlarge
  span.match(ng-repeat='filter in filters')
    span.close(ng-click="remove(filter)")
      i.icon-remove
    auto-input.field(result="filter", key="blank", suggestions="showList")
    span.comma :
    auto-input.value(result="filter.value", key="filter", suggestions="showList")
  auto-input.last(result="child", key="blank", suggestions="showList")